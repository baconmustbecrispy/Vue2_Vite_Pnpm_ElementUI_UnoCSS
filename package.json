{
  "name": "vue2vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue src",
    "format": "prettier --write .",
    "prepare": "husky install",
    "husky": "husky install && npx husky set .husky/pre-commit \"npx lint-staged\""
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@iconify-json/bxs": "^1.1.6",
    "@iconify-json/ic": "^1.1.13",
    "@unocss/preset-icons": "^0.53.1",
    "@unocss/preset-uno": "^0.53.1",
    "@unocss/transformer-directives": "^0.53.1",
    "@vitejs/plugin-legacy": "^4.0.4",
    "@vitejs/plugin-vue2": "^2.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "sass": "^1.63.3",
    "unocss": "^0.53.1",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vue-eslint-parser": "^9.3.1"
  },
  "dependencies": {
    "@unocss/reset": "^0.53.1",
    "element-ui": "^2.15.13",
    "generate-source-map": "^0.0.5",
    "rollup-plugin-require-context": "^1.0.1",
    "vite-plugin-vue2": "^2.0.3",
    "vue": "2.7.14",
    "vue-template-compiler": "^2.7.14"
  }
}
